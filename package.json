{
  "version": "0.1.0",
  "name": "cabana-ui",
  "description": "Cabana UI plugin for tailwindcss",
  "homepage": "https://cabana-ui.vercel.app/",
  "repository": "https://github.com/britzdylan/cabana-ui.git",
  "bugs": "https://github.com/britzdylan/cabana-ui/issues",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "keywords": [
    "design-system",
    "tailwindcss",
    "components",
    "ui-library",
    "component",
    "framework",
    "tailwind",
    "theming",
    "design",
    "css",
    "ui"
  ],
  "_moduleAliases": {
    "@": "."
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "link": "cd dist && npm link",
    "dev:local": "tsc && cd dist && npm link && cd .. && cd docs && npm link cabana-ui && npm run start",
    "publish": "tsc && cd dist && npm publish",
    "docs:start": "cd docs && npm run start",
    "docs:build": "cd docs && npm run build",
    "run:test": "node src/test.js"
  },
  "author": "Dylan Britz",
  "license": "Apache License",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "devDependencies": {
    "@types/node": "^18.16.3",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "module-alias": "^2.2.2"
  },
  "dependencies": {}
}
